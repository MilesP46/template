{
  "name": "@doctor-dok/shared-types",
  "version": "1.0.0",
  "private": true,
  "description": "Shared TypeScript types for Doctor Dok monorepo",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@doctor-dok/config": "*",
    "typescript": "^5.0.0"
  }
}